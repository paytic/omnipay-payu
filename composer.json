{
  "name": "bytic/omnipay-payu",
  "type": "library",
  "license": "MIT",
  "description": "PayU driver for ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "twispay",
    "driver",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/ByTIC/omnipay-payu",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
